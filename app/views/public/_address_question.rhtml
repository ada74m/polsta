<%= render :partial=>"public/question", :object=>address_question %>

<div class="response">
	<table>
	<% if address_question.line1usage != "not used" %>
	<tr>
		<td><%= h(address_question.line1label) %></td>
		<td><input name="<%= address_question.id %>:line1" value="<%= h(address_question.line1) %>" class="input" /></td>
	</tr>
	<% end %>
	<% if address_question.line2usage != "not used" %>
	<tr>
		<td><%= h(address_question.line2label) %></td>
