		<td><input name="<%= address_question.id %>:line3" value="<%= h(address_question.line3) %>" class="input" /></td>
	</tr>
	<% end %>
	<% if address_question.line4usage != "not used" %>
	<tr>
		<td><%= h(address_question.line4label) %></td>
		<td><input name="<%= address_question.id %>:line4" value="<%= h(address_question.line4) %>" class="input" /></td>
	</tr>
	<% end %>
	<% if address_question.line5usage != "not used" %>
	<tr>
		<td><%= h(address_question.line5label) %></td>
